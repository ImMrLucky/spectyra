# Server Configuration
PORT=8080

# Database (Postgres via Supabase)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Supabase Configuration
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_JWT_SECRET=your-jwt-secret-here

# Admin Access
ADMIN_TOKEN=your-admin-token-here

# LLM Provider Keys (for default providers, users can override via X-PROVIDER-KEY)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
GROK_API_KEY=

# Default Providers/Models
DEFAULT_TALK_PROVIDER=openai
DEFAULT_TALK_MODEL=gpt-4o-mini
DEFAULT_CODE_PROVIDER=anthropic
DEFAULT_CODE_MODEL=claude-3-5-sonnet-20241022

# Embeddings
OPENAI_EMBED_MODEL=text-embedding-3-small

# Pricing (per 1M tokens)
PRICE_OPENAI_INPUT=0.003
PRICE_OPENAI_OUTPUT=0.015
PRICE_ANTHROPIC_INPUT=0.003
PRICE_ANTHROPIC_OUTPUT=0.015
PRICE_GEMINI_INPUT=0.00025
PRICE_GEMINI_OUTPUT=0.001
PRICE_GROK_INPUT=0.001
PRICE_GROK_OUTPUT=0.002

# Optimizer Configuration
SIMILARITY_REUSE_THRESHOLD=0.90
STABILITY_T_LOW=0.35
STABILITY_T_HIGH=0.70
MAX_OUTPUT_TOKENS_OPTIMIZED=450
CODE_PATCH_MODE_DEFAULT=false

# Provider Key Fingerprint Salt (for audit trail)
PROVIDER_KEY_SALT=spectyra-audit-salt-change-in-production
